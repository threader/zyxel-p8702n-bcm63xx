BUILD_STATIC_MODULE_ARCHIVES= contrib/mod_sftp/mod_sftp.a
BUILD_STATIC_MODULE_OBJS=modules/mod_core.o modules/mod_xfer.o modules/mod_auth_unix.o modules/mod_auth_file.o modules/mod_auth.o modules/mod_ls.o modules/mod_log.o modules/mod_site.o modules/mod_delay.o modules/mod_facts.o modules/mod_ident.o   modules/mod_cap.o modules/module_glue.o

